include ./standard-blocks.jade

.container-fluid
  table.table.table-striped
    thead
      tr
        th Item
        th Value
    tbody
      tr(ng-repeat='stat in theRecords')
        td {{ stat.Name }}
        td {{ stat.Value }}

  +FormError
  +FormButtonGroup
    +FormButton('Refresh',{'ng-click':'fetch()'})
  
